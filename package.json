{
  "name": "ourdream",
  "version": "1.0.0",
  "description": "",
  "main": "server/app.js",
  "scripts": {
    "bower": "node_modules/.bin/bower install",
    "server-watch": "node_modules/.bin/babel --blacklist regenerator ./server/src --out-dir server/dist --watch ",
    "server-build": "node_modules/.bin/babel --blacklist regenerator ./server/src --out-dir server/dist",
    "client-watch": "node_modules/.bin/webpack --progress --colors -w --config ./webpack.config.js",
    "client-build": "node_modules/.bin/webpack --progress --colors -p --config ./webpack.config.js",
    "watch": "npm run server-watch | npm run client-watch",
    "build": "npm run server-build | npm run client-build",
    "dev": "node_modules/.bin/nodemon ./server/dist/app.js",
    "deploy": "npm run build && node_modules/.bin/pm2 -i -0 ./server/dist/app.js",
    "test": "node_modules/.bin/mocha -c -u bdd --require should --require should-http ./server/test/* --bail --harmony -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaokekeT/ourdream.git"
